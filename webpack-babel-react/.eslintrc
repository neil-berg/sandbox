{
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended"
    ],
    "plugins": ["prettier", "jsx-a11y"],
    "rules": {
        "prettier/prettier": ["off"],
        "jsx-filename-extension": 0,
        "jsx-quotes": ["error", "prefer-double"]
    }
}
